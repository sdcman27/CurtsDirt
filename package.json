{
  "name": "full-stack-app",
  "version": "1.0.0",
  "description": "A unified full-stack application with both frontend and backend.",
  "private": true,
  "main": "BackEnd/App.js",
  "type": "module",
  "scripts": {
    "dev": "cd FrontEnd && vite --port 3001",
    "build": "cd FrontEnd && vite build",
    "lint": "eslint FrontEnd --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "cd FrontEnd && vite preview",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-backend": "nodemon BackEnd/App.js",
    "start-frontend": " npm run dev",
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\""
  },
  "dependencies": {
    "@react-google-maps/api": "^2.19.3",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-auth-library": "^9.8.0",
    "googleapis": "^134.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-places-autocomplete": "^7.3.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nodemon": "^3.1.0",
    "vite": "^5.2.11"
  },
